    "import re \n",
    "def normalise_text(text):\n",
    "    words=text.split()\n",
    "    norm_text=' '.join(words)\n",
    "    norm_reg=re.sub(r'\\s+',' ',text.strip())\n",
    "    return norm_text,norm_reg\n",
    "text=\"the cute liitle cat   is playing    with juliee  ! .\"\n",
    "normalise_text(text)"
